@using MiniSurveys.Web.Models.UserView
@model IEnumerable<UserViewModel>

<table>
    <tr>
        <th>Логин</th>
        <th>Имя</th>
        <th>Фамилия</th>
        <th>Отчество</th>
        <th>E-mail</th>
        <th>Телефон</th>
        <th>Подразделение</th>
        <th>Роль</th>
        <th><img src="../img/pridor.svg" alt=""></th>
